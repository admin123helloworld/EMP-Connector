<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>For_Prospect_type_rating_should_not_Hot</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
 ISPICKVAL( Type ,&apos;Prospect&apos;),
 ISPICKVAL( Rating ,&apos;Hot&apos;) 
)</errorConditionFormula>
    <errorMessage>For Prospect type rating should not be Hot</errorMessage>
</ValidationRule>
