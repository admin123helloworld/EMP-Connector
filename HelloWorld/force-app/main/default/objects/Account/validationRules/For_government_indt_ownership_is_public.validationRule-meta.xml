<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>For_government_indt_ownership_is_public</fullName>
    <active>false</active>
    <errorConditionFormula>AND(

 ISPICKVAL( Industry ,&apos;Government&apos;),
 NOT(ISPICKVAL( Ownership ,&apos;Public&apos;)) 

)</errorConditionFormula>
    <errorMessage>For Government industry ownershp should be Public</errorMessage>
</ValidationRule>
