<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PAN_No_is_mandatory_for_manager_profile</fullName>
    <active>false</active>
    <errorConditionFormula>AND (

$Profile.Name=&apos;Manager&apos;,
 ISBLANK( PAN_No__c ) 

)</errorConditionFormula>
