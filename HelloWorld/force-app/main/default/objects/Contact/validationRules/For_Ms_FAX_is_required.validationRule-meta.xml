<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>For_Ms_FAX_is_required</fullName>
    <active>false</active>
    <errorConditionFormula>AND (

 ISPICKVAL( Salutation , &apos;Ms.&apos;),
 (ISBLANK( Fax ))

)</errorConditionFormula>
    <errorMessage>FAX is required for Ms.</errorMessage>
</ValidationRule>
