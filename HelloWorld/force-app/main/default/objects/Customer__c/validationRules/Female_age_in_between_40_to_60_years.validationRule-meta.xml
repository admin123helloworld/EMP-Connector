<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Female_age_in_between_40_to_60_years</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( Gender__c ,&apos;Female&apos;), 
Age__c &gt;=40,
Age__c &lt;60
)</errorConditionFormula>
    <errorMessage>Female customers age should not be in between 40 to 60 years</errorMessage>
</ValidationRule>
