<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Source_and_destination_city_should_diff</fullName>
    <active>true</active>
    <errorConditionFormula>TEXT( Source_location_City__c )= TEXT( Destination_City__c )</errorConditionFormula>
    <errorMessage>Source city and Destination city can not be same</errorMessage>
</ValidationRule>
